# Sorted and Cleaned Git and Linux Commands

        ## Directory and File Management
ls  
mkdir GIT_GitHub  
cd GIT_GitHub/  
touch tokyo.txt berlin.txt  
rm berlin.txt  
mv GIT_GitHub/ GIT-for-Devops  
mv GIT-for-Devops/ git-for-devops-batch-8  
cd git-for-devops-batch-8/  
vim tokyo.txt  
vim berlin.txt  
touch tatya.txt  

        ## Git Initialization and Configuration
git init  
git config --global user.name "Chetan"  
git config --global user.email "Chetan@gmail.com"  

        ## Git Status and Logs
git status  
git log  
git log --oneline  

        ## Adding and Committing Changes
git add tokyo.txt  
git add berlin.txt  
git add tatya.txt  
git rm --cached berlin.txt  
git commit -m "New files added"  
git commit -m "add one line in file"  
git commit -m "added test line"  
git commit -m "new 3rd file"  

        ## Branch Management
git branch  
git checkout -b dev  
git checkout master  
git checkout dev  

        ## Miscellaneous Git Commands
git restore berlin.txt  

        ## Viewing Files and Permissions
ls -a  
ls -la  

        ## Navigation and Clear Commands
cd  
cd 1.Batch-8-Server/  
clear  
history  

